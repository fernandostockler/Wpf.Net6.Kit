<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon></ApplicationIcon>
    <Authors>Fernando Stockler de Lima</Authors>
    <Description>A library with controls, styles and classes that let you start a WPF project with .Net6 easily and quickly.</Description>
    <PackageTags>wpf;custom-window;mvvm;wpf-control;</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Docs\Assets\CustomWindow_Mid_Blue_Gray_Left_Right.png" />
