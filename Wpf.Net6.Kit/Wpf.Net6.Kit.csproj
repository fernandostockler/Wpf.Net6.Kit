    <None Remove="Docs\Assets\CustomWindow_sample1_xaml.png" />
    <None Remove="Docs\Assets\CustomWindow_Small_White_Dialog_Green.png" />
    <None Remove="Docs\Assets\SideMenuItem\SideMenuItem_samplePage2.png" />
    <None Remove="Docs\Assets\SideMenuItem\SideMenuItem_sampleUserControl1.png" />
    <None Remove="Docs\Assets\SideMenu\SideMenu.png" />
    <None Remove="Docs\Assets\SideMenu\SideMenu_ItemSelected.png" />
    <None Remove="Docs\Assets\SideMenu\SideMenu_NoItemSelected_message.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="docfx.console" Version="2.58.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
